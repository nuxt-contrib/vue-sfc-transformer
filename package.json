{
  "name": "vue-sfc-transformer",
  "type": "module",
  "version": "0.1.11",
  "packageManager": "pnpm@10.8.1",
  "description": "Tools for minimal TypeScript transpilation of Vue SFCs",
  "license": "MIT",
  "repository": "nuxt-contrib/vue-sfc-transformer",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./mkdist": "./dist/mkdist.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.mts"
      ],
      "mkdist": [
        "./dist/mkdist.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint .",
    "prepare": "simple-git-hooks",
    "prepack": "pnpm build",
    "prepublishOnly": "pnpm lint && pnpm test",
    "release": "bumpp && pnpm publish",
    "test": "pnpm test:unit && pnpm test:types",
    "test:unit": "vitest",
    "test:knip": "knip",
    "test:versions": "installed-check -d --no-workspaces",
    "test:types": "tsc --noEmit"
  },
  "peerDependencies": {
    "esbuild": "*",
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@babel/parser": "^7.27.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.12.0",
    "@babel/types": "7.27.0",
    "@types/node": "22.14.1",
    "@vitest/coverage-v8": "3.1.1",
    "@vue/compiler-dom": "3.5.13",
    "bumpp": "10.1.0",
    "changelogithub": "13.13.0",
    "esbuild": "0.25.2",
    "eslint": "9.25.0",
    "exsolve": "1.0.5",
    "installed-check": "9.3.0",
    "knip": "5.50.5",
    "lint-staged": "15.5.1",
    "mkdist": "2.3.0",
    "pkg-pr-new": "0.0.42",
    "simple-git-hooks": "2.12.1",
    "typescript": "5.8.3",
    "unbuild": "3.5.0",
    "vitest": "3.1.2",
    "vue": "3.5.13",
    "vue-tsc": "2.2.8"
  },
  "resolutions": {
    "vue-sfc-transformer": "link:."
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,mjs,cjs,json,.*rc}": [
      "npx eslint --fix"
    ]
  }
}
