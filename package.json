{
  "name": "vue-sfc-transformer",
  "type": "module",
  "version": "0.1.16",
  "packageManager": "pnpm@10.11.0",
  "description": "Tools for minimal TypeScript transpilation of Vue SFCs",
  "license": "MIT",
  "repository": "nuxt-contrib/vue-sfc-transformer",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./mkdist": "./dist/mkdist.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.mts"
      ],
      "mkdist": [
        "./dist/mkdist.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint .",
    "prepare": "simple-git-hooks",
    "prepack": "pnpm build",
    "prepublishOnly": "pnpm lint && pnpm test",
    "release": "bumpp && pnpm publish",
    "test": "pnpm test:unit && pnpm test:types",
    "test:unit": "vitest",
    "test:knip": "knip",
    "test:versions": "installed-check -d --no-workspaces",
    "test:types": "tsc --noEmit"
  },
  "peerDependencies": {
    "@vue/compiler-core": "^3.5.13",
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@babel/parser": "^7.27.0",
    "oxidase": "^0.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.13.2",
    "@babel/types": "7.27.1",
    "@types/node": "22.15.21",
    "@vitest/coverage-v8": "3.1.4",
    "@vue/compiler-core": "3.5.15",
    "@vue/compiler-dom": "3.5.15",
    "bumpp": "10.1.1",
    "changelogithub": "13.14.0",
    "eslint": "9.27.0",
    "exsolve": "1.0.5",
    "installed-check": "9.3.0",
    "knip": "5.58.1",
    "lint-staged": "16.0.0",
    "mkdist": "2.3.0",
    "pkg-pr-new": "0.0.50",
    "simple-git-hooks": "2.13.0",
    "typescript": "5.8.3",
    "unbuild": "3.5.0",
    "vitest": "3.1.4",
    "vue": "3.5.16",
    "vue-tsc": "2.2.10"
  },
  "resolutions": {
    "@vue/compiler-core": "3.5.15",
    "vue-sfc-transformer": "link:."
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,mjs,cjs,json,.*rc}": [
      "npx eslint --fix"
    ]
  }
}
