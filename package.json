{
  "name": "vue-sfc-transformer",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.7.0",
  "description": "",
  "license": "MIT",
  "repository": "nuxt-contrib/vue-sfc-transformer",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "prepack": "pnpm build",
    "prepublishOnly": "pnpm lint && pnpm test",
    "release": "bumpp && pnpm publish",
    "test": "pnpm test:unit && pnpm test:types",
    "test:unit": "vitest",
    "test:knip": "knip",
    "test:versions": "installed-check -d --no-workspaces",
    "test:types": "tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@babel/generator": "^7.27.0",
    "@babel/parser": "^7.27.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@babel/types": "^7.27.0",
    "@types/babel__generator": "^7.27.0",
    "@vitest/coverage-v8": "latest",
    "@vue/compiler-dom-types": "npm:@vue/compiler-dom@^3.5.13",
    "bumpp": "latest",
    "changelogithub": "latest",
    "esbuild": "^0.25.2",
    "eslint": "latest",
    "exsolve": "^1.0.4",
    "installed-check": "latest",
    "knip": "latest",
    "lint-staged": "latest",
    "simple-git-hooks": "latest",
    "typescript": "latest",
    "unbuild": "latest",
    "vitest": "latest",
    "vue": "^3.5.13"
  },
  "resolutions": {
    "vue-sfc-transformer": "link:."
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,mjs,cjs,json,.*rc}": [
      "npx eslint --fix"
    ]
  }
}
